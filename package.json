  "scripts": {
    "inspect": "kill-port --port 9229 && node --inspect=0.0.0.0:9229 build/startup.js",
    "start_watch_inspect": "nodemon --delay 80ms --watch build/ build/startup.js --exec 'npm run inspect'",
    "start-prod": "ts-node-dev --transpile-only --respawn --ignore-watch node_modules src/main/server.ts",
    "start": "nodemon --legacy-watch src/main/server.ts",
    "test": "jest"
  },
  "dependencies": {
    "express": "^4.17.3",
    "tsc": "^2.0.4",
    "uuidv4": "^6.2.12"
  },
  "devDependencies": {
    "@prisma/client": "^3.11.1",
    "@swc/core": "^1.2.151",
    "@swc/jest": "^0.2.20",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/supertest": "^2.0.12",
    "dotenv": "^16.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prisma": "^3.11.0",
    "supertest": "^6.2.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  }
}
