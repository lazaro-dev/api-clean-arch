{
  "name": "lazaro",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "inspect": "kill-port --port 9229 && node --inspect=0.0.0.0:9229 build/startup.js",
    "start_watch_inspect": "nodemon --delay 80ms --watch build/ build/startup.js --exec 'npm run inspect'",
    "start-prod": "ts-node-dev --transpile-only --respawn --ignore-watch node_modules src/server.ts",
    "start": "nodemon --legacy-watch src/server.ts"
  },
  "dependencies": {
    "express": "^4.17.3",
    "tsc": "^2.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.15",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  }
}
